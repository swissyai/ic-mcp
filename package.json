{
  "name": "icp-mcp",
  "version": "0.6.0",
  "description": "Model Context Protocol server for Internet Computer development",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "icp-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsx src/index.ts",
    "start": "node dist/index.js",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:integration": "vitest run tests/integration",
    "test:unit": "vitest run tests/unit",
    "clean": "rm -rf dist"
  },
  "keywords": [
    "mcp",
    "internet-computer",
    "icp",
    "motoko",
    "rust",
    "candid",
    "dfinity",
    "model-context-protocol"
  ],
  "author": "swissyai (https://x.com/swissyai)",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/swissyp/icp-mcp.git"
  },
  "devDependencies": {
    "@types/node": "^24.9.1",
    "@vitest/ui": "^4.0.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.2"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.20.1",
    "@octokit/rest": "^22.0.0",
    "cheerio": "^1.1.2",
    "node-fetch": "^3.3.2",
    "turndown": "^7.2.1",
    "zod": "^3.25.76"
  }
}
